<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="VladimirPopov_WebForms::webforms" title="Web-forms" sortOrder="10">
                        <resource id="VladimirPopov_WebForms::manage_forms" title="Manage Forms" sortOrder="100"/>
                        <resource id="VladimirPopov_WebForms::quickresponse" title="Manage Quickresponse" sortOrder="105"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="VladimirPopov_WebForms::settings" title="Web-forms Settings" sortOrder="2000"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
